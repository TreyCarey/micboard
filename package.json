    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@ibm/plex": "^5.1.3",
    "@popperjs/core": "^2.9.2",
    "@shopify/draggable": "^1.0.0-beta.8",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
